{% block %}
    <div class="container">
    <div class="row">
        <div class="col-md-4 col-lg-4"></div>
        <div class="col-md-4 col-lg-4 ">
            <div id="form-login">
            <p>{{ 'registration.confirmed'|trans({'%username%': user.username}) }}</p>
            {% if targetUrl %}
            <p><a href="{{ targetUrl }}">{{ 'registration.back'|trans }}</a></p>
            {% endif %}
            </div>
        </div>
        <div class="col-md-4 col-lg-4"></div>
    </div>
</div>
{% endblock %}
